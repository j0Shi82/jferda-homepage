{
  "i18n-ally.localesPaths": ["src/locale", "coverage/lcov-report/locale", "src/assets/media/images/skills/language"],
  "editor.quickSuggestions": {
    "strings": true
  },
  "[svelte]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.codeActionsOnSave": ["source.sortImports", "source.formatDocument", "source.fixAll.format"]
  },
  "[javascript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.codeActionsOnSave": ["source.sortImports", "source.formatDocument", "source.fixAll.format"]
  },
  "npm.packageManager": "pnpm",
  "eslint.useFlatConfig": true
}
